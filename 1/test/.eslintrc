{
    "parser": "@babel/eslint-parser",
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    },
    "globals": {
      "expect": true
    },
    "ignorePatterns": ["public/"],
    "plugins": ["import"],
    "extends": [
      "eslint:recommended",
      "google",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "rules": {
      "indent": [2, 4, {"SwitchCase": 1}],
      "max-len": ["error", 120],
      "require-jsdoc": "off",
      "comma-dangle": "off",
      "no-console": 2
    }
  }